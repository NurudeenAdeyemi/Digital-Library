@model Core.DTOs.AuthorModel

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Admin";
}

<dl class="row">
    <dt class="col-sm-2">
        First Name:
    </dt>
    <dd class="col-sm-10">
        @Model.FirstName
    </dd>
    <dt class="col-sm-2">
        Last Name:
    </dt>
    <dd class="col-sm-10">
        @Model.LastName
    </dd>
    <dt class="col-sm-2">
        Biography:
    </dt>
    <dd class="col-sm-10">
        @Model.Biography
    </dd>
    
</dl>

<button style="background-color:darkblue;"><a asp-action="Index">Back to List</a></button>